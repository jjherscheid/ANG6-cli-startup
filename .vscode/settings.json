// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.DS_Store": true,
        "**/node_modules/**": true,
        "**/typings/**": true,
        "**/dist/**": true,
        "**/testresults/**": true,
        "**/npm-debug.log": true
    },
    "tslint.enable": true,
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "files.hotExit": "off"
}